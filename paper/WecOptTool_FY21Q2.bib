	issn = {1070-9908},
	journal = {IEEE Signal Processing Letters},
	keywords = {filters;mean square error methods;- filter;IEEEXplore;Matlab;aeolotropic filter design;arbitrary observable systems;dynamic models;mean squared error;optimal fixed-gain filters;Kalman filters;Mathematical model;Noise measurement;Q measurement;Riccati equations;Signal processing algorithms;Filtering;linear systems;optimal control},
	month = {July},
