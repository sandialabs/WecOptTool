	groups = {AdvWecCntrls Pubs, [rcoe:]},
	issn = {2214-1669},
	journal = {International Journal of Marine Energy},
	keywords = {Wave energy conversion, Parametric excited wave energy conversion, Pitch-surge control, Model Predictive Control},
