@article{drew_review_2009,
	author = {Drew, Benjamin and Plummer, A. R. and Sahinkaya, M. Necip},
	date-modified = {2020-12-15 11:02:33 -0700},
	doi = {10.1243/09576509JPE782},
	file = {[PDF] from washington.edu:/Users/mlawson/Library/Application Support/Zotero/Profiles/hg7lotd0.default/zotero/storage/VSP9EPD2/Drew et al. - 2009 - A review of wave energy converter technology.pdf:application/pdf;Snapshot:/Users/mlawson/Library/Application Support/Zotero/Profiles/hg7lotd0.default/zotero/storage/JQ9TS8WW/887.html:text/html},
