@article{Abraham2013,
	author = {Abraham, E. and Kerrigan, E.C.},
	doi = {10.1109/TSTE.2012.2224392},
	file = {Abraham2013.pdf:pdfs\\Abraham2013.pdf:PDF},
	issn = {1949-3029},
	journal = {Sustainable Energy, IEEE Transactions on},
	keywords = {gradient methods;nonlinear programming;optimal control;power generation control;quadratic programming;wave power generation;bang-bang type;general nonlinear program;irregular sea conditions;linear dampers;optimal active control schemes;optimal command latching method;power maximization problem;power takeoff mechanism;projected gradient optimization scheme;quadratic program;standard NLP solver;wave energy converter optimization;Approximation methods;Force;Linear programming;Optimal control;Optimization;Switches;Vectors;Optimal control;projected gradient method;wave energy},
	month = {April},
	number = {2},
	owner = {gbacelli},
	pages = {324-332},
	timestamp = {2014.07.01},
	title = {Optimal Active Control and Optimization of a Wave Energy Converter},
	volume = {4},
	year = {2013},
	abstract = {This paper investigates optimal active control schemes applied to a point absorber wave energy converter within a receding horizon fashion. A variational formulation of the power maximization problem is adapted to solve the optimal control problem. The optimal control method is shown to be of a bang-bang type for a power takeoff mechanism that incorporates both linear dampers and active control elements. We also consider a direct transcription of the optimal control problem as a general nonlinear program. A variation of the projected gradient optimization scheme is formulated and shown to be feasible and computationally inexpensive compared to a standard NLP solver. Since the system model is bilinear and the cost function is not convex quadratic, the resulting optimization problem is not a quadratic program. Results will be compared with an optimal command latching method to demonstrate the improvement in absorbed power. All time domain simulations are generated under irregular sea conditions.},
	Bdsk-Url-1 = {https://doi.org/10.1109/TSTE.2012.2224392}}

@phdthesis{Alves2012,
	author = {Alves, Marco Aurelio},
	file = {Alves2012.pdf:pdfs\\Alves2012.pdf:PDF},
	school = {Technical University of Lisbon},
	title = {Numerical Simulation of the Dynamics of Point Absorber Wave Energy Converters Using Frequency and Time Domain Approaches},
	type = {{PhD}},
	year = {2012},
	abstract = {The first objective of this work was the development of numerical tools, to analyse the dynamics of Wave Energy Converters {WECs}, based on frequency and time domain approaches. The frequency model developed is based on an entirely linear hydrodynamic interaction between the {WEC} and ocean waves as well as any external force on the {WEC}, including the force impinged by the power-take-off equipment {PTO}, which is fairly acceptable within the device operational regime where the waves and device oscillations are, in general, described by low amplitude motions. So, in the frequency domain, all the physical quantities vary sinusoidally with time, according to the frequency of the sinusoidal incident wave. Thus, the model allows to describe the device dynamics in terms of motion (velocities or accelerations) complex amplitudes for specified linear {PTO} characterizations, and, consequently, to estimate the mean power absorption. However, in many practical cases, the {WEC} dynamics has some parts which are strongly non-linear, and so, the superposition principle is no longer applicable. Those cases may be the nonlinear forces induced by the mooring system and the {PTO} designed for a specific control strategy. Hence, a correct analysis of the problem has to be done in the time domain. For this reason a time domain model covering the entire energy conversion chain (wave-to-wire model) was developed by means of a generic approach in order to extend its applicability to study different technologies. Besides the development of numerical tools, an application to analyse the dynamics of an axisymmetric floating {OWC} was also part of this work. Therefore, the frequency model was used to characterize the device in terms of shape and size, since one of the main model outputs is the mean absorption level. From this study several considerations related to the design of heaving axisymmetric point absorbers came out. Moreover, the wave-to-wire code was applied to study the dynamics response of the floating {OWC} under two different control strategies, in order to bound the device capability to absorb wave energy. The first one consists of a {PTO} whose performance is equivalent to a damper of uniform coefficient, which corresponds to the poorest energy absorption state. Further, a control strategy based on an entirely predictive control was also implemented in order to characterize the device performance under the most suitable energy absorption condition. This approach enables a clear idea to be obtained about the quality of any further control strategy applied.},
	annote = {Read up to pg. 108}}

@inproceedings{Alves2011,
	address = {Southampton, {UK}},
	author = {Alves, Marco and Sarmento, Ant{\~{A}}\textsuperscript{3}nio and Vicente, Miguel and Guerinel, Mathieul},
	booktitle = {Proceedings of the 9\textsuperscript{th} European Wave and Tidal Energy Conference ({EWTEC} 2011)},
	file = {Alves2011.pdf:pdfs\\Alves2011.pdf:PDF},
	month = sep,
	title = {Implementation and Verification of a Time Domain Model to Simulate the Dynamics of {OWCs}},
	url = {http://www.see.ed.ac.uk/~shs/EWTEC%202011%20full/EWTEC_CD/papers/246.pdf},
	urldate = {2013-12-12},
	year = {2011},
	abstract = {The present paper reports a study concerning the methodologies followed to build a time domain model to describe the entire dynamics of a {WEC} (wave-to-wire model). In essence the time domain model is based on the second Newton{\^{a}}{\texteuro}{\texttrademark}s law, which states that the inertial force is balanced by the whole forces acting over the {WEC{\^{a}}{\texteuro}{\texttrademark}s} captor. Thus, the entire force is decomposed into hydrodynamic and external sources. The hydrodynamic source comprises the excitation force, inflicted by the incident waves, the buoyancy and the radiation force. In addition, the device motion is also constrained by external forces induced by both the {PTO} equipment and the mooring system. Besides the model implementation, some results concerning the code verification are presented. The code verification has been done by using analytical solutions of simple cases, as the dynamics of cylinders, along with the knowledge of the behavior of floating bodies in some particular conditions as, for instance, the dynamic response under regular waves for the limiting cases of infinite and zero frequency.},
	Bdsk-Url-1 = {http://www.see.ed.ac.uk/~shs/EWTEC%202011%20full/EWTEC_CD/papers/246.pdf}}

