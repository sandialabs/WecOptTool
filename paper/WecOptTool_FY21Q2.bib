@article{henderson_design_2006,
	author = {Henderson, Ross},
	doi = {10.1016/j.renene.2005.08.021},
	file = {ScienceDirect Snapshot:C\:\\Users\\jleonqu\\Zotero\\storage\\K7GZ9DHG\\S0960148105002259.html:text/html},
	issn = {0960-1481},
	journal = {Renewable Energy},
