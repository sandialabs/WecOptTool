	journal = {IEEE Transactions on Industry Applications},
	keywords = {adaptive control;power convertors;adaptive vector control;wave energy converters;grid connection concerns;maximum power absorption;power quality;WEC control concept;near-commercial deployments;maximum energy extraction;dominant frequency characteristics;maximum power extraction conditions;Force;Absorption;Frequency locked loops;Real-time systems;Mathematical model;Energy resources;Time-domain analysis;Adaptive control;wave energy},
