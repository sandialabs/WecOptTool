@article{Alnajjab2017,
	author = {B. Alnajjab and R. S. Blum},
	doi = {10.1109/TSTE.2016.2590567},
	file = {Alnajjab2017.pdf:pdfs\\Alnajjab2017.pdf:PDF;Alnajjab2017.pdf:.\\pdfs\\Alnajjab2017.pdf:PDF},
	groups = {Estimation},
	issn = {1949-3029},
	journal = {IEEE Transactions on Sustainable Energy},
	keywords = {Gaussian noise;distributed sensors;linear programming;matrix algebra;mean square error methods;ocean waves;parameter estimation;wave power generation;white noise;CRB;Cramer-Rao bound;Fisher information matrix;WEC device control;linear optimizations;low-complexity estimation method;mean-square estimation error;ocean energy conversion efficiency enhancement;ocean waves;parametric estimation;periodograms;phase unwrapping;sensor network;spatially distributed ocean sensors;wave energy converter;waveform estimation;white Gaussian noise;Estimation;Noise measurement;Ocean waves;Oceans;Sea measurements;Sensors;Time measurement;Parameter estimation;short-term forecasting;wave energy;wave forecasting},
	month = {Jan},
	number = {1},
	owner = {gbacell},
	pages = {179-191},
	timestamp = {2017.04.02},
	title = {Estimating Waveforms of Ocean Waves to Enhance the Efficiency of Ocean Energy Conversion},
	volume = {8},
	year = {2017},
	abstract = {Advances in the area of active controls for wave energy converter (WEC) devices have uncovered a need for accurate estimates of the actual waves that will arrive at the WEC devices in the near future, typically several seconds ahead. This paper considers the parametric estimation of these impending waves by employing a network of spatially distributed ocean sensors, possibly of different types, whose measurements are noisy. We provide general expressions for the Fisher information matrix and the Cramer-Rao Bound (CRB) under white Gaussian noise. The CRB is a lower bound on the smallest mean-square estimation error, which is nearly achievable for a sufficient number of observations. The expressions are general in the sense that the sensor network could be made up of a single or multiple types of sensors. We present a low-complexity estimation method that employs linear optimizations coupled with periodograms and phase unwrapping. Numerical results demonstrating the accuracy of the presented estimation method relative to the CRB are provided.},
	Bdsk-Url-1 = {https://doi.org/10.1109/TSTE.2016.2590567}}

@article{Crouse2015,
	author = {D. F. Crouse},
