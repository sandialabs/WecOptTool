@inproceedings{Bull2012,
	address = {Hampton Roads, VA},
	author = {Bull, Diana and Jacob, Paul},
	booktitle = {{OCEANS} '12. '{Harnessing} the {Power} of the {Ocean}'. {Proceedings}},
	doi = {10.1109/OCEANS.2012.6404870},
	file = {Bull2012.pdf:pdfs\\Bull2012.pdf:PDF},
	keywords = {BBDB, Drag, Equations, extreme wave environment, Floods, Force, Hydrodynamics, Mathematical model, mooring, Morison Equation, OrcaFlex, OWC, Surges, WEC},
	month = oct,
	owner = {dlbull},
	pages = {1 --9},
	timestamp = {2015.05.11},
	title = {Methodology for creating nonaxisymmetric {WECs} to screen mooring designs using a {Morison} {Equation} approach},
	year = {2012},
	abstract = {This paper outlines a methodology to derive the rigid body motions from Morison Equation forces for a floating Backward Bent Duct Buoy (BBDB). This Wave Energy Converter (WEC) is a particular style of Oscillating Water Column (OWC). The methodology details how to decompose the BBDB into a series of \#x2018;hydrodynamic bodies \#x2019; to account for inertial and viscous effects, \#x2018;structural bodies \#x2019; to account for structural mass and inertia, and freely flooding bodies to account for the time dependent variation of entrained water mass in the OWC of the device. Separating these effects allows for independent treatment of the phenomena being modeled and for an easily adaptable WEC model. Because effects of the PTO can be eliminated when considering extreme conditions, OrcaFlex, a validated mooring time domain solver, is applied to this problem. Thus coupled analysis between the proposed mooring system design and the BBDB dynamic response allows for mooring system component specification. Due to the dependence of Morison Equation force magnitudes on empirically derived drag coefficients, the paper concludes with a study quantifying the effect of drag coefficient magnitude on survival mooring loads.},
	annote = {Dorian Brefort
Bull and Jacob, Methodology for Creating Nonaxisymmetric WECs to Screen Mooring Designs using a Morison Equation ApproachWEC mooring is a major part, so important not to over-design it.BBDB highest theoretical conversion efficiency due to relative motion between internal free surface and structureDiffraction ignored --{\textgreater} Morison's Equation method appliedDimensions of BBDB must always insure that diffraction parameter is smallLinear summation of forces on a reference body to measure total force and kinematics. Virtual plates with drag and added mass terms to account for motionsand force due to hydrodynamic force. Buoyancy chamber only provide lifting force, but not affected by hydrodynamic loads. Entrained water columns for time dependent additional massVery little research on added mass of large amplitude motion devices in steady flow, but studies show dependence of Cd and Ca on KC number. Here Cd and Ca are considered independent of frequencyDevice considered a perfect wave follower to get baseline parameters. (I'm assuming you mean hydrodynamic parameters, but it might be a good idea to specify). Unlikely to have Cd and Cm data for shape of WEC, so further studies necessaryHigher discretisation of model will produce more accurate fundamental response of model. We should maybe also consider the fundamental response of the WEC+mooring system, but it is not as important in survivability conditions as in regular conditions.Study peak loads as function of Drag coefficients to make method adaptable.Description of model, and of parametric drag study. Might want to add in a directional study for survivability.},
	Bdsk-Url-1 = {https://doi.org/10.1109/OCEANS.2012.6404870}}

@inproceedings{Bull2013,
	address = {Aalborg, Denmark},
	author = {Bull, Diana and Johnson, Eric},
	booktitle = {Proceedings of the 10\textsuperscript{th} European Wave and Tidal Energy Conference},
	file = {Bull2013.pdf:pdfs\\Bull2013.pdf:PDF},
	publisher = {Technical Committee of the European Wave and Tidal Energy Conference},
	title = {Optimal Resistive Control Strategy for a Floating {OWC} Device},
	url = {http://energy.sandia.gov/wp/wp-content/gallery/uploads/SAND2013-6198c.pdf},
	year = {2013},
	abstract = {A resistive control strategy to optimize pneumatic power for a floating {OWC} device is presented. This strategy utilizes a linear, frequency-domain performance model that links an oscillating structure to air-pressure fluctuations with a Wells Turbine in 3-dimensions. An array of field points defining the interior free surface allows hydrodynamic parameters relating to the fluctuating air-pressure within the {OWC} to be calculated using reciprocity relations. Device structural parameters for a non-optimized {BBDB} are detailed and the performance model is exercised on this device. A new resonance results from coupling the floating structure to the air-column that is unique from the uncoupled resonance location. An analytic expression for the optimal resistive load to link the floating structure and aircolumn dynamics is presented. When the optimal resistive load is exercised within the model, the natural resonances of the coupled system are preserved and additional linked peaks are identified. This formulation of the optimal resistive load is shown to contribute significantly to the device capture width and power performance.},
	Bdsk-Url-1 = {http://energy.sandia.gov/wp/wp-content/gallery/uploads/SAND2013-6198c.pdf}}

