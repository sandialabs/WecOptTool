	doi = {10.1007/s00773-003-0168-0},
	file = {Kashiwagi2004.pdf:pdfs\\Kashiwagi2004.pdf:PDF;Snapshot:pdfs\\Kashiwagi2004.html:URL},
	issn = {0948-4280, 1437-8213},
	journal = {Journal of Marine Science and Technology},
	keywords = {Hydroelastic response, Landing, Take-off, Time domain, Very large floating structure ({VLFS)}},
	language = {en},
	month = may,
	number = {1},
	pages = {14--23},
	title = {Transient responses of a {VLFS} during landing and take-off of an airplane},
	urldate = {2014-02-27},
	volume = {9},
	year = {2004},
	abstract = {The transient elastic deformation of a pontoon-type very large floating structure ({VLFS)} caused by the landing and take-off of an airplane is computed by the time-domain mode-expansion method. The memory effects in hydrodynamic forces are taken into account, and great care is paid to numerical accuracy in evaluating all the coefficients appearing in the simultaneous differential equations for the elastic motion of a {VLFS.} The time-histories of the imparted force and the position and velocity of an airplane during landing and take-off are modeled with data from a Boeing 747-400 jumbo jet. Simulation results are shown of 3-D structural waves on a {VLFS} and the associated unsteady drag force on an airplane, which is of engineering importance, particularly during take-off. The results for landing show that the airplane moves faster than the structural waves generated in the early stage, and the waves overtake the airplane as its speed decreases to zero. The results for take-off are essentially the same as those for landing, except that the structural waves develop slowly in the early stage, and no obstacle exists on the runway after the take-off of airplane. The additional drag force on an airplane due to the elastic responses of the runway considered in this work was found to be small in magnitude.},
	Bdsk-Url-1 = {https://doi.org/10.1007/s00773-003-0168-0}}

@inproceedings{Kaspenberg2010,
	address = {Pasadena, Ca},
	author = {Kaspenberg, G. K. and Thornhill, E. T.},
	booktitle = {Proceedings of the 28\textsuperscript{th} Symposium on Naval Hydrodynamics},
	file = {Kaspenberg2010.pdf:pdfs\\Kaspenberg2010.pdf:PDF},
	keywords = {{CFD} wave validation, slamming},
	title = {A Practical Approach to Ship Slamming in Waves},
	year = {2010},
	abstract = {Bow flare slamming can cause very large impulse loadings to a ship{\^{a}}{\texteuro}{\texttrademark}s structure that can result in local damage or severe whipping responses that may adversely affect the ship{\^{a}}{\texteuro}{\texttrademark}s ultimate strength and fatigue life in a seaway. The current work is intended to develop a fast and accurate method capable of long term simulations of ship in a seaway from which statistical properties of impact loads can be derived. This procedure includes an approximation method based on Momentum Theory enhanced with pile-up effects due to the immersing bow and the draft dependent static bow wave. It is shown that the latter effects are crucial for a good prediction of the impact force. The method is tuneable to specific ship characteristics, but it captures the effect of different wave conditions and different headings well. The tuning can be done using a dedicated set of experiments, either in the towing tank or by {CFD.} The {CFD} option is illustrated with a calculation in head seas showing a very good agreement of the force on the bow and of local pressures to experiments.},
	annote = {Used {CFD} and experiments to tune momentum-theory-based model. Comparison with experimental results was fair to good. Odd phasing issue unexplained or mentioned.}}

