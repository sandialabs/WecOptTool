	file = {Korde2000.pdf:pdfs\\Korde2000.pdf:PDF},
	keywords = {Absorption, Automatic control, control system applications, control system synthesis, Control systems, Damping, deep-water floating devices, device dynamics active control, Energy conversion, Frequency domain analysis, frequency domain control, frequency-domain synthesis, Hydrodynamics, latching, on-board reaction, Oscillators, power station control, Productivity, reactive control, Read, secondary converters, Time domain analysis, time-domain control, wave energy conversion, wave energy devices, wave power generation, wave power plants},
